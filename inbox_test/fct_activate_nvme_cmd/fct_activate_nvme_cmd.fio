#do not modify the section name
[global]
ioengine=libaio
direct=1
buffered=0
size=100%
continue_on_error=none
filename=/dev/nvme0n1

[precondition_write]
iodepth=256
bs=128k
rw=write
offset=0
do_verify=0
verify=pattern
verify_dump=1
verify_fatal=1
verify_backlog=1
verify_pattern=0x55aa55aa
numjobs=1
thread
stonewall

[seqrd]
iodepth=256
bs=${BS}
rw=read
offset=0
time_based
runtime=60
do_verify=1
verify=pattern
verify_dump=1
verify_fatal=1
verify_backlog=1
verify_pattern=0x55aa55aa
numjobs=1
thread
