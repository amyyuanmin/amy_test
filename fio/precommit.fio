verify_dump=1
verify_fatal=1
verify_backlog=1
time_based
runtime=300
stonewall

[seqrd_256k_iodepth_128]
bs=256k
iodepth=128
rw=read
filename=/dev/nvme0n1
time_based
runtime=300
stonewall

[randwr_4k_iodepth_128]
